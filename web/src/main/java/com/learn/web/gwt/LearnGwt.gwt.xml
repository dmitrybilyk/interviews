<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="learnGwt">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.google.gwt.activity.Activity"/>
    <inherits name="com.google.gwt.place.Place"/>
    <inherits name="com.google.gwt.inject.Inject"/>
    <inherits name="com.google.gwt.inject.Inject"/>
    <!--    <inherits name="com.google.gwt.junit.JUnit"/>-->

    <source path="client"/>
    <source path="shared"/>
<!--    <source path='test'/>-->

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.learn.web.gwt.client.LearnGwt'/>
<!--    activity-->
<!--    <entry-point class='com.learn.web.gwt.client.activity.HelloMVP'/>-->

    <!-- Use ClientFactoryImpl by default -->
    <replace-with class="com.learn.web.gwt.client.activity.ClientFactoryImpl">
        <when-type-is class="com.learn.web.gwt.client.activity.ClientFactory"/>
    </replace-with>

    <!--<stylesheet src='MySampleApplication.css' />-->

    <!-- Specify the app servlets.                   -->
    <!--<servlet path='/MySampleApplicationService' class='com.mySampleApplication.server.MySampleApplicationServiceImpl'/>-->
    <!--<servlet path='/DepartmentsServiceGWT' class='com.mySampleApplication.server.DepartmentsServiceGWTImpl'/>-->

</module>